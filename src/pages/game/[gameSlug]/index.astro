---
import Layout from '../../../layouts/Layout.astro';
import GameViewRouter from '../../../components/GameViewRouter.tsx';

export const prerender = false;

const { gameSlug } = Astro.params;

if (!gameSlug) {
  return Astro.redirect('/dashboard');
}
---

<Layout title="Game View - NFL Pickem">
	<GameViewRouter gameSlug={gameSlug} client:load />
</Layout>